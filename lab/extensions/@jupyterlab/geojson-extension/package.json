{
  "name": "@jupyterlab/geojson-extension",
  "version": "3.1.2",
  "description": "GeoJSON renderer for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyter-renderers",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyter-renderers/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyter-renderers.git"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Project Jupyter",
    "email": "jupyter@googlegroups.com"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.*",
    "style/index.js"
  ],
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension && jlpm run clean:dist",
    "clean:labextension": "rimraf ./jupyterlab-geojson/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:dist": "rimraf dist",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepack": "npm run clean && npm run build",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/apputils": "^3.0.5",
    "@jupyterlab/rendermime-interfaces": "^3.0.5",
    "@lumino/messaging": "^1.4.3",
    "@lumino/widgets": "^1.16.1",
    "leaflet": "^1.5.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "@types/leaflet": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "jupyterlab": {
    "mimeExtension": true,
    "outputDir": "jupyterlab-geojson/labextension",
    "_build": {
      "load": "static/remoteEntry.a6d92e11d5e8c375cfe6.js",
      "mimeExtension": "./mimeExtension",
      "style": "./style"
    }
  },
  "styleModule": "style/index.js"
}
